			<dataPath>ES_ПолучательКонтактноеЛицо</dataPath>
			<field>ES_ПолучательКонтактноеЛицо</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ES_ЗаказчикКонтактноеЛицо</dataPath>
			<field>ES_ЗаказчикКонтактноеЛицо</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ES_Курьер</dataPath>
			<field>ES_Курьер</field>

					И РеквизитЗаказа = ЗНАЧЕНИЕ(Перечисление.ES_ИзменяемыеРеквизитыЗаказа.ФИОПолучателя)) КАК ES_ИзменяемыеРеквизитыЗаказовСрезПоследнихФИОПолучателя
		ПО ES_ДанныеПоЗаказамСрезПоследних.Заказ = ES_ИзменяемыеРеквизитыЗаказовСрезПоследнихФИОПолучателя.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ES_РасходыПоЗаказам.Обороты(
				,
				,
				,
				Заказ В
					(ВЫБРАТЬ
						ВТ_Заказы.Заказ
					ИЗ
						ВТ_Заказы КАК ВТ_Заказы)) КАК ES_РасходыПоЗаказамОбороты
		ПО ES_ДанныеПоЗаказамСрезПоследних.Заказ = ES_РасходыПоЗаказамОбороты.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ES_СведенияОЗаборах.СрезПоследних КАК ES_СведенияОЗаборахСрезПоследних
		ПО ES_ДанныеПоЗаказамСрезПоследних.Заказ = ES_СведенияОЗаборахСрезПоследних.ОснованиеЗабора
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ES_СтатусыЗаказовСрезПоследних.Заказ КАК Заказ,
	ЕСТЬNULL(ES_СтатусыЗаказовСрезПоследних.СтатусЗаказа, ЗНАЧЕНИЕ(Перечисление.ES_СтатусыЗаказов.ПустаяСсылка)) КАК СтатусЗаказа
ПОМЕСТИТЬ ВТ_Статусы
ИЗ
	РегистрСведений.ES_СтатусыЗаказов.СрезПоследних(
			{(&amp;ТекДата)},
			Заказ В
				(ВЫБРАТЬ
					ВТ_Заказы.Заказ
				ИЗ
					ВТ_Заказы КАК ВТ_Заказы)) КАК ES_СтатусыЗаказовСрезПоследних
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫРАЗИТЬ(Продажи.Регистратор КАК Документ.РасходнаяНакладная) КАК РасходнаяНакладная,
	Продажи.ЗаказПокупателя КАК ЗаказПокупателя
ПОМЕСТИТЬ ВТ_РН
ИЗ
	РегистрНакопления.Продажи КАК Продажи
ГДЕ
	Продажи.Регистратор ССЫЛКА Документ.РасходнаяНакладная

СГРУППИРОВАТЬ ПО
	Продажи.ЗаказПокупателя,
	ВЫРАЗИТЬ(Продажи.Регистратор КАК Документ.РасходнаяНакладная)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Док.Заказчик КАК Заказчик,
	ВТ_Док.Заказ КАК Заказ,
	ВТ_Док.ВидДоставки КАК ВидДоставки,
	ВТ_Док.НППлан КАК НППлан,
	ВТ_Док.СтоимостьДоставки КАК СтоимостьДоставки,
	ВТ_Док.Плательщик КАК Плательщик,
	ВТ_Док.КоличествоМест КАК КоличествоМест,
	ВТ_Док.ОбщийВес КАК ОбщийВес,
	ВТ_Док.Получатель КАК Получатель,
	ВТ_Док.АдресДоставки КАК АдресДоставки,
	ВТ_Док.ДатаДоставки КАК ДатаДоставки,
	ВТ_Док.СрочностьДоставки КАК СрочностьДоставки,
	ВТ_Док.НомерНакладной КАК НомерНакладной,
	NULL КАК Расходы,
	ВЫРАЗИТЬ(ВТ_Док.Комментарий КАК СТРОКА(100)) КАК Комментарий,
	ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставкуОбороты.УдалитьФактическиеНПОборот, 0) КАК НПФакт,
	ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставкуОбороты.ФактическиеНП_НалОборот, 0) КАК НПФактНал,
	ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставкуОбороты.ФактическиеНП_БезналОборот, 0) КАК НПФактБезнал,
	ЕСТЬNULL(ОплатаСчетовИЗаказовОбороты.СуммаАвансаОборот, 0) + ЕСТЬNULL(ОплатаСчетовИЗаказовОбороты.СуммаОплатыОборот, 0) КАК Оплачено,
	ЕСТЬNULL(ВТ_Док.СтоимостьДоставки, 0) - (ЕСТЬNULL(ОплатаСчетовИЗаказовОбороты.СуммаАвансаОборот, 0) + ЕСТЬNULL(ОплатаСчетовИЗаказовОбороты.СуммаОплатыОборот, 0)) КАК НеОплачено,
	ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставкуОбороты.СуммаВозвращеноОборот, 0) КАК Возвращено,
	ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставкуОбороты.СуммаУдержаноОборот, 0) КАК Удержано,
	ЕСТЬNULL(ВТ_Статусы.СтатусЗаказа, ЗНАЧЕНИЕ(Перечисление.ES_СтатусыЗаказов.ПустаяСсылка)) КАК СтатусЗаказа,
	ВТ_Док.Курьер КАК Курьер,
	ВТ_Док.СуммаПоДоставке КАК СуммаПоДоставке,
	ВТ_Док.СуммаДопУслуги КАК СуммаДопУслуги,
	ЕСТЬNULL(ВТ_Док.ВидОплаты, ВТ_Док.Заказ.ES_ВидОплаты) КАК ВидОплаты,
	ВТ_Док.Подрядчик КАК Подрядчик,
	ВТ_Док.СправочноЗаказчик КАК СправочноЗаказчик,
	ВТ_Док.ВхНакладная КАК ВхНакладная,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВТ_Статусы.СтатусЗаказа, ЗНАЧЕНИЕ(Перечисление.ES_СтатусыЗаказов.ПустаяСсылка)) = ЗНАЧЕНИЕ(Перечисление.ES_СтатусыЗаказов.Отказ)
			ТОГДА ВТ_Док.ДатаОтмены
		ИНАЧЕ ВТ_Док.ДатаВыполнения
	КОНЕЦ КАК ДатаВыполнения,
	ES_ЗаказыНаСкладеОстатки.Склад КАК Склад,
	ВТ_Док.ВидЗабора КАК ВидЗабора,
	ВТ_РН.РасходнаяНакладная КАК РасходнаяНакладная,
	ВТ_Док.ЧекНаОбщуюСумму КАК ЧекНаОбщуюСумму,
	ВТ_Док.ЗачетНП КАК ЗачетНП,
	ВТ_Док.ДоУточнения КАК ДоУточнения,
	ВТ_Док.Направление КАК Направление,
	ВТ_Док.Проблема КАК Проблема,
	ВТ_Док.Поврежден КАК Поврежден,
	ВТ_Док.УслугиПартнера КАК УслугиПартнера,
	ВТ_Док.ОбщийОбъем КАК ОбщийОбъем,
	ВТ_Док.ОбщийОбъемныйВес КАК ОбщийОбъемныйВес,
	ВТ_Док.КомментарииСтатусов КАК КомментарииСтатусов,
	ВТ_Док.ФИОПолучателя КАК ФИОПолучателя,
	ВТ_Док.ЗаказES_ВнутризональныйКоэф КАК ES_ВнутризональныйКоэф,
	ВТ_Док.ЗаказES_ЗонаДоставки КАК ES_ЗонаДоставки,
	ВТ_Док.ЗаказES_ОбьявленнаяСтоимость КАК ES_ОбьявленнаяСтоимость,
	ВТ_Док.РасходыДоставки КАК РасходыДоставки,
	ЕСТЬNULL(ES_РасходыПоЗаказамОбороты.СуммаОборот, 0) КАК РасходыПоСбору
ИЗ
	ВТ_Док КАК ВТ_Док
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ES_УчетДСпоЗаказамНаДоставку.Обороты(
				{(&amp;ПустаяДата)},
				{(&amp;ТекДата)},
				,
				Заказ В
					(ВЫБРАТЬ
						ВТ_Заказы.Заказ
					ИЗ
						ВТ_Заказы КАК ВТ_Заказы)) КАК ES_УчетДСпоЗаказамНаДоставкуОбороты
		ПО ВТ_Док.Заказ = ES_УчетДСпоЗаказамНаДоставкуОбороты.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОплатаСчетовИЗаказов.Обороты(
				{(&amp;ПустаяДата)},
				{(&amp;ТекДата)},
				,
				СчетНаОплату В
					(ВЫБРАТЬ
						ВТ_Заказы.Заказ
					ИЗ
						ВТ_Заказы КАК ВТ_Заказы)) КАК ОплатаСчетовИЗаказовОбороты
		ПО ВТ_Док.Заказ = ОплатаСчетовИЗаказовОбороты.СчетНаОплату
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Статусы КАК ВТ_Статусы
		ПО ВТ_Док.Заказ = ВТ_Статусы.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ES_ЗаказыНаСкладе.Остатки(
				,
				Заказ В
					(ВЫБРАТЬ
						ВТ_Заказы.Заказ
					ИЗ
						ВТ_Заказы КАК ВТ_Заказы)) КАК ES_ЗаказыНаСкладеОстатки
		ПО ВТ_Док.Заказ = ES_ЗаказыНаСкладеОстатки.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_РН КАК ВТ_РН
		ПО ВТ_Док.Заказ = ВТ_РН.ЗаказПокупателя
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ES_РасходыПоЗаказам.Обороты(
				,
				,
				,
				Заказ В
					(ВЫБРАТЬ
						ВТ_Док.Забор КАК Забор
					ИЗ
						ВТ_Док КАК ВТ_Док)) КАК ES_РасходыПоЗаказамОбороты
		ПО ВТ_Док.Забор = ES_РасходыПоЗаказамОбороты.Заказ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ЗаказПокупателя.Контрагент,
	ЗаказПокупателя.Ссылка,
	ЗаказПокупателя.ES_ВидДоставки,
	ЗаказПокупателя.ES_НППлан,
	ЗаказПокупателя.ES_СтоимостьДоставки,
	ЗаказПокупателя.ES_Плательщик,
	ЗаказПокупателя.ES_КоличествоМест,
	ЗаказПокупателя.ES_ОбщийВес,
	ЗаказПокупателя.ES_Получатель,
	ЗаказПокупателя.ES_АдресДоставки,
	ЗаказПокупателя.ES_ДатаДоставки,
	ЗаказПокупателя.ES_СрочностьДоставки,
	ЗаказПокупателя.ES_НомерНакладной,
	ЗаказПокупателя.ES_Расходы,
	ВЫРАЗИТЬ(ЗаказПокупателя.Комментарий КАК СТРОКА(100)),
	0,
	0,
	0,
	0,
	0,
	0,
	0,
	ЗНАЧЕНИЕ(Перечисление.ES_СтатусыЗаказов.ПустаяСсылка),
	ЗаказПокупателя.ES_Курьер,
	0,
	0,
	ЗаказПокупателя.ES_ВидОплаты,
	ЗаказПокупателя.ES_Подрядчик,
	ЗаказПокупателя.ES_СправочноЗаказчик,
	ЗаказПокупателя.ES_ВхНакладная,
	NULL,
	ЗаказПокупателя.ES_Склад,
	ЗаказПокупателя.ES_ВидЗабора,
	ЗНАЧЕНИЕ(Документ.РасходнаяНакладная.ПустаяСсылка),
	ЗаказПокупателя.ES_ЧекНаОбщуюСумму,
	ЗаказПокупателя.ES_ЗачетНП,
	ЗаказПокупателя.ES_ДоУточнения,
	ЗаказПокупателя.ES_Направление,
	ЗаказПокупателя.ES_Проблема,
	ЛОЖЬ,
	0,
	ЗаказПокупателя.ES_Объем,
	ЗаказПокупателя.ES_ОбщийОбъемныйВес,
	ЗНАЧЕНИЕ(Справочник.ES_КомментарииКСтатусам.ПустаяСсылка),
	NULL,
	ЗаказПокупателя.ES_ВнутризональныйКоэф,
	ЗаказПокупателя.ES_ЗонаДоставки,
	ЗаказПокупателя.ES_ОбьявленнаяСтоимость,
	0,
	0
ИЗ
	Документ.ЗаказПокупателя КАК ЗаказПокупателя
ГДЕ
	ЗаказПокупателя.ES_ДатаДоставки МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И НЕ ЗаказПокупателя.Проведен
	И НЕ ЗаказПокупателя.ПометкаУдаления</query>

		<field xsi:type="DataSetFieldField">
			<dataPath>СтоимостьДоставки</dataPath>
			<field>СтоимостьДоставки</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОбщийВес</dataPath>
			<field>ОбщийВес</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Вес, кг</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Забор</dataPath>
			<field>Забор</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОтчетОДоставкеСтатус</dataPath>
			<field>ОтчетОДоставкеСтатус</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Состояние</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ES_СведенияОЗаказах.ПланДоставки КАК ПланДоставки,
	ES_СведенияОЗаказах.Заказ КАК Заказ
ПОМЕСТИТЬ ВТ_ПланыИЗаказы
ИЗ
	РегистрСведений.ES_СведенияОЗаказах КАК ES_СведенияОЗаказах
ГДЕ
	ES_СведенияОЗаказах.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ES_СведенияОЗаказах.Регистратор ССЫЛКА Документ.ES_ПланДоставки

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ES_СведенияОЗаборах.ПланДоставки,
	ES_СведенияОЗаборах.Забор
ИЗ
	РегистрСведений.ES_СведенияОЗаборах КАК ES_СведенияОЗаборах
ГДЕ
	ES_СведенияОЗаборах.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ES_СведенияОЗаборах.Регистратор ССЫЛКА Документ.ES_ПланДоставки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ПланыИЗаказы.ПланДоставки КАК ПланДоставки
ПОМЕСТИТЬ ВТ_СписокПланов
ИЗ
	ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы

СГРУППИРОВАТЬ ПО
	ВТ_ПланыИЗаказы.ПланДоставки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СписокПланов.ПланДоставки КАК ПланДоставки,
	ЕСТЬNULL(ВТ_СписокПланов.ПланДоставки.Курьер, ES_СвязьПлановИОтчетовСрезПоследних.Курьер) КАК Курьер,
	ЕСТЬNULL(ES_СвязьПлановИОтчетовСрезПоследних.ОтчетОДоставке, НЕОПРЕДЕЛЕНО) КАК ОтчетОДоставке,
	ЕСТЬNULL(ES_СвязьПлановИОтчетовСрезПоследних.ОтчетОДоставке.Обработан, ЛОЖЬ) КАК ОтчетОДоставкеОбработан,
	ЕСТЬNULL(ES_СвязьПлановИОтчетовСрезПоследних.РасходИзОтчета, 0) КАК ОтчетОДоставкеРасход,
	ES_СвязьПлановИОтчетовСрезПоследних.ОтчетОДоставке.Статус КАК ОтчетОДоставкеСтатус
ПОМЕСТИТЬ ВТ_ПланИОтчет
ИЗ
	ВТ_СписокПланов КАК ВТ_СписокПланов
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ES_СвязьПлановИОтчетов.СрезПоследних(
				,
				ПланДоставки В
					(ВЫБРАТЬ
						ВТ_СписокПланов.ПланДоставки
					ИЗ
						ВТ_СписокПланов КАК ВТ_СписокПланов)) КАК ES_СвязьПлановИОтчетовСрезПоследних
		ПО ВТ_СписокПланов.ПланДоставки = ES_СвязьПлановИОтчетовСрезПоследних.ПланДоставки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВложенныйЗапрос.Курьер КАК Курьер
ПОМЕСТИТЬ ВТ_Заказ_Курьер
ИЗ
	(ВЫБРАТЬ
		ES_СвязьПлановИОтчетовСрезПоследних.Курьер КАК Курьер,
		ES_ПланДоставкиЗаказы.ДокументДоставки КАК ДокументДоставки
	ИЗ
		РегистрСведений.ES_СвязьПлановИОтчетов.СрезПоследних КАК ES_СвязьПлановИОтчетовСрезПоследних
			ЛЕВОЕ СОЕДИНЕНИЕ Документ.ES_ПланДоставки.Заказы КАК ES_ПланДоставкиЗаказы
			ПО ES_СвязьПлановИОтчетовСрезПоследних.ПланДоставки = ES_ПланДоставкиЗаказы.Ссылка
	
	СГРУППИРОВАТЬ ПО
		ES_СвязьПлановИОтчетовСрезПоследних.Курьер,
		ES_ПланДоставкиЗаказы.ДокументДоставки) КАК ВложенныйЗапрос
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ПланыИЗаказы.ПланДоставки КАК ПланДоставки,
	ВТ_ПланыИЗаказы.Заказ КАК Заказ,
	ЕСТЬNULL(ES_ДанныеПоЗаказамСрезПоследних.НомерНакладной, """") КАК НомерНакладной,
	ЕСТЬNULL(ES_ДанныеПоЗаказамСрезПоследних.АдресДоставки, """") КАК АдресДоставки,
	ЕСТЬNULL(ES_ДанныеПоЗаказамСрезПоследних.ПолучательРегион, НЕОПРЕДЕЛЕНО) КАК Регион,
	ES_ДанныеПоЗаказамСрезПоследних.СтоимостьДоставки КАК СтоимостьДоставки,
	ES_ДанныеПоЗаказамСрезПоследних.ОбщийВес КАК ОбщийВес
ПОМЕСТИТЬ ВТ_ДанныеПоЗаказам
ИЗ
	ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ES_ДанныеПоЗаказам.СрезПоследних(
				,
				Заказ В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)) КАК ES_ДанныеПоЗаказамСрезПоследних
		ПО ВТ_ПланыИЗаказы.Заказ = ES_ДанныеПоЗаказамСрезПоследних.Заказ
ГДЕ
	ВТ_ПланыИЗаказы.Заказ ССЫЛКА Документ.ЗаказПокупателя

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_ПланыИЗаказы.ПланДоставки,
	ВТ_ПланыИЗаказы.Заказ,
	ЕСТЬNULL(ES_ДанныеПоЗаборамСрезПоследних.НомерНакладной, """"),
	ЕСТЬNULL(ES_ДанныеПоЗаборамСрезПоследних.Адрес, """"),
	ЕСТЬNULL(ES_ДанныеПоЗаборамСрезПоследних.Регион, НЕОПРЕДЕЛЕНО),
	NULL,
	ES_ДанныеПоЗаборамСрезПоследних.ОбщийВес
ИЗ
	ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ES_ДанныеПоЗаборам.СрезПоследних(
				,
				Забор В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)) КАК ES_ДанныеПоЗаборамСрезПоследних
		ПО ВТ_ПланыИЗаказы.Заказ = ES_ДанныеПоЗаборамСрезПоследних.Забор
ГДЕ
	ВТ_ПланыИЗаказы.Заказ ССЫЛКА Документ.ES_ЗаборГруза
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ДанныеПоЗаказам.ПланДоставки КАК ПланДоставки,
	ВТ_ПланИОтчет.Курьер КАК Курьер,
	ВТ_ПланИОтчет.ОтчетОДоставке КАК ОтчетОДоставке,
	ВТ_ПланИОтчет.ОтчетОДоставкеОбработан КАК ОтчетОДоставкеОбработан,
	ВТ_ПланИОтчет.ОтчетОДоставкеРасход КАК ОтчетОДоставкеРасход,
	ВТ_ДанныеПоЗаказам.Заказ КАК Заказ,
	ВТ_ДанныеПоЗаказам.НомерНакладной КАК НомерНакладной,
	ВТ_ДанныеПоЗаказам.АдресДоставки КАК АдресДоставки,
	ВТ_ДанныеПоЗаказам.Регион КАК Регион,
	ВТ_ДанныеПоЗаказам.СтоимостьДоставки КАК СтоимостьДоставки,
	ВТ_ДанныеПоЗаказам.ОбщийВес КАК ОбщийВес,
	ВТ_ПланИОтчет.ОтчетОДоставкеСтатус КАК ОтчетОДоставкеСтатус
ПОМЕСТИТЬ Итоговая
ИЗ
	ВТ_ДанныеПоЗаказам КАК ВТ_ДанныеПоЗаказам
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПланИОтчет КАК ВТ_ПланИОтчет
		ПО ВТ_ДанныеПоЗаказам.ПланДоставки = ВТ_ПланИОтчет.ПланДоставки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Итоговая.ПланДоставки КАК ПланДоставки,
	Итоговая.Курьер КАК Курьер,
	Итоговая.ОтчетОДоставке КАК ОтчетОДоставке,
	Итоговая.ОтчетОДоставкеОбработан КАК ОтчетОДоставкеОбработан,
	Итоговая.ОтчетОДоставкеРасход КАК ОтчетОДоставкеРасход,
	Итоговая.Заказ КАК Заказ,
	Итоговая.НомерНакладной КАК НомерНакладной,
	Итоговая.АдресДоставки КАК АдресДоставки,
	ВложенныйЗапрос.СтатусЗаказа КАК СтатусЗаказа,
	Итоговая.Регион КАК Регион,
	КомментарийИзОтчета.Комментарий КАК Комментарий,
	РасходИзСвязиПлановИОтчетов.РасходИзОтчета КАК РасходИзОтчета,
	НалБезналИзУЧетаДС.ФактСумма_Нал КАК ФактСумма_Нал,
	НалБезналИзУЧетаДС.ФактСумма_Безнал КАК ФактСумма_Безнал,
	ЕСТЬNULL(Итоговая.СтоимостьДоставки, 0) КАК СтоимостьДоставки,
	Итоговая.ОбщийВес КАК ОбщийВес,
	Итоговая.ОтчетОДоставкеСтатус КАК ОтчетОДоставкеСтатус
ПОМЕСТИТЬ ВТ_Итоговая
ИЗ
	Итоговая КАК Итоговая
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ES_СтатусыЗаказов.Заказ КАК Заказ,
			МАКСИМУМ(ES_СтатусыЗаказов.СтатусЗаказа) КАК СтатусЗаказа,
			ЕСТЬNULL(ES_СтатусыЗаказов.Регистратор.Курьер, ES_СтатусыЗаказов.Ответственный) КАК Курьер,
			ES_СтатусыЗаказов.Регистратор КАК Регистратор
		ИЗ
			РегистрСведений.ES_СтатусыЗаказов КАК ES_СтатусыЗаказов
		ГДЕ
			ES_СтатусыЗаказов.Заказ В
					(ВЫБРАТЬ
						ВТ.Заказ
					ИЗ
						Итоговая КАК ВТ)
		
		СГРУППИРОВАТЬ ПО
			ES_СтатусыЗаказов.Заказ,
			ЕСТЬNULL(ES_СтатусыЗаказов.Регистратор.Курьер, ES_СтатусыЗаказов.Ответственный),
			ES_СтатусыЗаказов.Регистратор
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ES_СтатусыЗаборов.Забор,
			ES_СтатусыЗаборов.СтатусЗабора,
			ЕСТЬNULL(ES_СтатусыЗаборов.Регистратор.Курьер, ES_СтатусыЗаборов.Ответственный),
			ES_СтатусыЗаборов.Регистратор
		ИЗ
			РегистрСведений.ES_СтатусыЗаборов КАК ES_СтатусыЗаборов
		ГДЕ
			ES_СтатусыЗаборов.Забор В
					(ВЫБРАТЬ
						Итоговая.Заказ
					ИЗ
						Итоговая КАК Итоговая)) КАК ВложенныйЗапрос
		ПО Итоговая.Курьер = ВложенныйЗапрос.Курьер
			И Итоговая.Заказ = ВложенныйЗапрос.Заказ
			И (ВЫБОР
				КОГДА Итоговая.ОтчетОДоставке = НЕОПРЕДЕЛЕНО
					ТОГДА Итоговая.ПланДоставки = ВложенныйЗапрос.Регистратор
				ИНАЧЕ Итоговая.ОтчетОДоставке = ВложенныйЗапрос.Регистратор
			КОНЕЦ)
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ES_ИзменяемыеРеквизитыЗаказов.Значение КАК Комментарий,
			ES_ИзменяемыеРеквизитыЗаказов.Регистратор КАК КомментарийОтчет,
			ES_ИзменяемыеРеквизитыЗаказов.Заказ КАК Заказ
		ИЗ
			РегистрСведений.ES_ИзменяемыеРеквизитыЗаказов КАК ES_ИзменяемыеРеквизитыЗаказов
		ГДЕ
			ES_ИзменяемыеРеквизитыЗаказов.РеквизитЗаказа = ЗНАЧЕНИЕ(Перечисление.ES_ИзменяемыеРеквизитыЗаказа.КомментарийСтатуса)
			И ES_ИзменяемыеРеквизитыЗаказов.Заказ В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ES_ИзменяемыеРеквизитыЗаборов.Значение,
			ES_ИзменяемыеРеквизитыЗаборов.Регистратор,
			NULL
		ИЗ
			РегистрСведений.ES_ИзменяемыеРеквизитыЗаборов КАК ES_ИзменяемыеРеквизитыЗаборов
		ГДЕ
			ES_ИзменяемыеРеквизитыЗаборов.Забор В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)
			И ES_ИзменяемыеРеквизитыЗаборов.РеквизитЗабора = ЗНАЧЕНИЕ(Перечисление.ES_ИзменяемыеРеквизитыЗабора.Комментарий)) КАК КомментарийИзОтчета
		ПО Итоговая.ОтчетОДоставке = КомментарийИзОтчета.КомментарийОтчет
			И Итоговая.Заказ = КомментарийИзОтчета.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ЕСТЬNULL(ES_ИзменяемыеРеквизитыЗаказов.Значение, 0) КАК Расход,
			ES_ИзменяемыеРеквизитыЗаказов.Регистратор КАК Регистратор
		ИЗ
			РегистрСведений.ES_ИзменяемыеРеквизитыЗаказов КАК ES_ИзменяемыеРеквизитыЗаказов
		ГДЕ
			ES_ИзменяемыеРеквизитыЗаказов.РеквизитЗаказа = ЗНАЧЕНИЕ(Перечисление.ES_ИзменяемыеРеквизитыЗаказа.Расходы)
			И ES_ИзменяемыеРеквизитыЗаказов.Заказ В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ЕСТЬNULL(ES_ИзменяемыеРеквизитыЗаборов.Значение, 0),
			ES_ИзменяемыеРеквизитыЗаборов.Регистратор
		ИЗ
			РегистрСведений.ES_ИзменяемыеРеквизитыЗаборов КАК ES_ИзменяемыеРеквизитыЗаборов
		ГДЕ
			ES_ИзменяемыеРеквизитыЗаборов.РеквизитЗабора = ЗНАЧЕНИЕ(Перечисление.ES_ИзменяемыеРеквизитыЗабора.Расходы)
			И ES_ИзменяемыеРеквизитыЗаборов.Забор В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)) КАК РасходИзОтчета
		ПО Итоговая.ОтчетОДоставке = РасходИзОтчета.Регистратор
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ES_СвязьПлановИОтчетов.РасходИзОтчета КАК РасходИзОтчета,
			ES_СвязьПлановИОтчетов.ОтчетОДоставке КАК ОтчетОДоставке
		ИЗ
			РегистрСведений.ES_СвязьПлановИОтчетов КАК ES_СвязьПлановИОтчетов
		ГДЕ
			ES_СвязьПлановИОтчетов.ПланДоставки В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.ПланДоставки
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)) КАК РасходИзСвязиПлановИОтчетов
		ПО Итоговая.ОтчетОДоставке = РасходИзСвязиПлановИОтчетов.ОтчетОДоставке
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ES_УчетДСпоЗаказамНаДоставку.Регистратор КАК Регистратор,
			ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставку.ФактСумма_Нал, 0) КАК ФактСумма_Нал,
			ЕСТЬNULL(ES_УчетДСпоЗаказамНаДоставку.ФактСумма_Безнал, 0) КАК ФактСумма_Безнал,
			ES_УчетДСпоЗаказамНаДоставку.Заказ КАК Заказ
		ИЗ
			РегистрНакопления.ES_УчетДСпоЗаказамНаДоставку КАК ES_УчетДСпоЗаказамНаДоставку
		ГДЕ
			ES_УчетДСпоЗаказамНаДоставку.Заказ В
					(ВЫБРАТЬ
						ВТ_ПланыИЗаказы.Заказ
					ИЗ
						ВТ_ПланыИЗаказы КАК ВТ_ПланыИЗаказы)) КАК НалБезналИзУЧетаДС
		ПО Итоговая.ОтчетОДоставке = НалБезналИзУЧетаДС.Регистратор
			И Итоговая.Заказ = НалБезналИзУЧетаДС.Заказ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ES_НастройкаДоступаПользователей.Сотрудник КАК Сотрудник
ПОМЕСТИТЬ ВТ_ПартнерскиеКурьеры
ИЗ
	РегистрСведений.ES_НастройкаДоступаПользователей КАК ES_НастройкаДоступаПользователей
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Итоговая.ПланДоставки КАК ПланДоставки,
	ВТ_Итоговая.Курьер КАК Курьер,
	ВТ_Итоговая.ОтчетОДоставке КАК ОтчетОДоставке,
	ВТ_Итоговая.ОтчетОДоставкеОбработан КАК ОтчетОДоставкеОбработан,
	ВТ_Итоговая.ОтчетОДоставкеРасход КАК ОтчетОДоставкеРасход,
	ВТ_Итоговая.Заказ КАК Заказ,
	ВТ_Итоговая.НомерНакладной КАК НомерНакладной,
	ВТ_Итоговая.АдресДоставки КАК АдресДоставки,
	ВТ_Итоговая.СтатусЗаказа КАК СтатусЗаказа,
	ВТ_Итоговая.Регион КАК Регион,
	ВТ_Итоговая.Комментарий КАК Комментарий,
	ВТ_Итоговая.РасходИзОтчета КАК РасходИзОтчета,
	ВТ_Итоговая.ФактСумма_Нал КАК ФактСумма_Нал,
	ВТ_Итоговая.ФактСумма_Безнал КАК ФактСумма_Безнал,
	ВТ_Итоговая.СтоимостьДоставки КАК СтоимостьДоставки,
	ВТ_Итоговая.ОбщийВес КАК ОбщийВес,
	ВТ_Итоговая.ОтчетОДоставкеСтатус КАК ОтчетОДоставкеСтатус
ИЗ
	ВТ_Итоговая КАК ВТ_Итоговая
ГДЕ
	НЕ ВТ_Итоговая.Курьер В
				(ВЫБРАТЬ
					ВТ_ПартнерскиеКурьеры.Сотрудник
				ИЗ
					ВТ_ПартнерскиеКурьеры КАК ВТ_ПартнерскиеКурьеры)</query>
	</dataSet>
